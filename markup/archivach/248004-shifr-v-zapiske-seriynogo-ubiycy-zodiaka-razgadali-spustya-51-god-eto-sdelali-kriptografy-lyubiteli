<air script="">
  (function(w, d, images) {
    var teaser = d.querySelector('.vk-results-of-the-year');

    loadImages(images).then(function() {
      teaser.classList.add('vk-results-of-the-year--loaded');
    });

    function loadImage(url) {
      var img = d.createElement('img');
      return new Promise(function(resolve) {
        img.onload = resolve;
        img.src = url;
      });
    };

    function loadImages(urls) {
      return Promise.all(urls.map(function(url) {
        return loadImage(url);
      }));
    };

  }(window, document, ['https://leonardo.osnova.io/ed739b2f-0e84-59df-9049-bd6462077dd2/', 'https://leonardo.osnova.io/3364c5fa-f51b-504a-8c2d-d3cddc80ec7c/', 'https://leonardo.osnova.io/be4a4cec-855b-5d34-b109-517c222af2a3/', 'https://leonardo.osnova.io/14c04196-6900-581c-a8c9-b459a8a08d20/', 'https://leonardo.osnova.io/f3d16731-3812-55b5-98ee-577986554f79/']));
